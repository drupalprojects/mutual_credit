<?php

function mcapi_import_enable() {
  $link = array(
    'link_path' => 'admin/mc/import',
    'link_title' => 'Import',
    'module' => 'mcapi_import',
    'menu_name' => 'setup',
    'weight' => '50',
  );
  menu_link_save($link);
}

function mcapi_import_disable() {
  mcapi_delete_menu_link();
}
